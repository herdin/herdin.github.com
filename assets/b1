/*BLOCK CHAIN*/
package study.blockchain.bitcoin;

import java.math.BigDecimal;
import java.security.NoSuchAlgorithmException;
import java.util.ArrayList;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import study.blockchain.BlockChainUtils;

class BlockHeader {
    public Long version;
    public byte[] previousBlockHash;
    public byte[] merkletRootHash;
    public Long timestamp;
    public Long nBits;
    public Long nonce;
    @Override
    public String toString() {
        StringBuffer sb = new StringBuffer();
        sb
        //version : numeric -> byte array Big-endian -> byte array Little-endian -> hex String
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(BlockChainUtils.numericToHexString(this.version, 4)))))
        //previousBlockHash : byte array Big-endian -> byte array Little-endian -> hex String 
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(this.previousBlockHash)))
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(this.merkletRootHash)))
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(BlockChainUtils.numericToHexString(this.timestamp, 4)))))
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(BlockChainUtils.numericToHexString(this.nBits, 4)))))
        .append(BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(BlockChainUtils.numericToHexString(this.nonce, 4)))))
        ;
        return sb.toString();
    }//END OF FUNCTION
}//END OF CLASS

public class Block {
    private int blockSize;
    private BlockHeader blockHeader;
    private int transactionCount;
    private Object[] transaction;
    
    public Block(BlockHeader blockHeader, String[] transactions) {
        this.blockHeader = blockHeader;
        this.transaction = transaction;
    }//END OF FUNCTION
    
    public byte[] getBlockHash() throws NoSuchAlgorithmException {
        return BlockChainUtils.getReversedByteArray(BlockChainUtils.encSHA256(BlockChainUtils.encSHA256(BlockChainUtils.hexStringToByteArray(this.blockHeader.toString()))));
    }//END OF FUNCTION
    
