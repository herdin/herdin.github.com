            class MerkleTree {
                private boolean isDebug = true;
                private String[] hashes;
                BlockHeader header;
                public MerkleTree(String[] hashes, String merkleRootHash) {
                    this.hashes = hashes;
                    this.header = new BlockHeader();
                    header.merkletRootHash = BlockChainUtils.hexStringToByteArray(merkleRootHash);
                }
//              def merkle_path(index): 
//              header={}
//              header['merkle_root']='f68cd8ad6571d29c91dbfea769a6a834ed50f231650406b4e62eefac958c6317'
//              proof=mk_merkle_proof(header, hashes, index)
//              siblings=proof['siblings']
//              print 'merkle path : ', siblings
//              target_tx=hashes[index]
//              siblings = map( lambda x: x.decode('hex')[::-1], siblings)
//              resultHash = target_tx.decode('hex')[::-1]
//              proofLen = len(siblings)
//              i = 0
//              while i < proofLen:
//                  proofHex = siblings[i]
//                  sideOfSibling = index % 2  # 0 means sibling is on the right; 1 means left
//                  if sideOfSibling == 1:
//                      left = proofHex
//                      right = resultHash
//                  elif sideOfSibling == 0:
//                      left = resultHash
//                      right = proofHex
//                  resultHash = bitcoin.bin_sha256(bitcoin.bin_sha256(left + right  ))
//                  index /= 2
//                  i += 1
//              assert resultHash[::-1].encode('hex') == header['merkle_root']
                public boolean merkle_path(int index) {
                    this.merkle_proof(this.header, this.hashes, index);
                    
                    return false;
                }//END OF FUNCTION
                
//                slice는 start:stop[:step]의 형식으로 쓸 수있습니다. 여기서 [:step]은 써도 되고 안써도 된다는 의미입니다.
//
//                step을 명시하지 않을 경우에는
//
//                a[start:end] # start부터 end-1까지의 item
//                a[start:] # start부터 리스트 끝까지 item
//                a[:end] # 처음부터 end-1까지의 item
//                a[:] # 리스트의 모든 item
//                step value를 쓰는 경우에는
//
//                a[start:end:step]# start부터 end-1까지 step만큼 인덱스 증가시키면서
//                step을 지정할 때 :end에 유의하세요 end는 end부터 포함시키지 않겠다는 의미이지 end가 꼭 포함된다는 의미는 아닙니다.
//
//                또 start나 end가 음수가 음수인 경우에는 리스트의 끝에서부터 카운트하겠다는 의미입니다.
//
//                a[-1] # 맨 뒤의 item
//                a[-2:] # 맨 뒤에서부터 item2개
//                a[:-n] # 맨 뒤의 item n개 빼고 전부
                
//              def mk_merkle_proof(header, hashes, index):
//              nodes = [h.decode('hex')[::-1] for h in hashes] //헥사스트링을 헥사로 바꾸고 엔디안변환후 nodes 에 저장
//              if len(nodes) % 2 and len(nodes) > 2: //nodes 길이가 짝수면서 길이가 2보다 크면
//                  nodes.append(nodes[-1])
//              layers = [nodes]
//              while len(nodes) > 1:
//                  newnodes = []
//                  for i in range(0, len(nodes) - 1, 2):
//                      newnodes.append(bitcoin.bin_sha256(bitcoin.bin_sha256(nodes[i] + nodes[i+1])))
//                  if len(newnodes) % 2 and len(newnodes) > 2:
//                      newnodes.append(newnodes[-1])
//                  nodes = newnodes
//                  layers.append(nodes)
//              # Sanity check, make sure merkle root is valid
//              assert nodes[0][::-1].encode('hex') == header['merkle_root']
//              merkle_siblings = \
//                  [layers[i][(index >> i) ^ 1] for i in range(len(layers)-1)]
//              return {
//                  "hash": hashes[index],
//                  "siblings": [x[::-1].encode('hex') for x in merkle_siblings],
//                  "header": header
//              }
                public void merkle_proof(BlockHeader header, String[] hashes, int index) {
                    try {
                        if(this.isDebug) System.out.println("merkle_proof start.");
                        ArrayList<byte[]> nodes = new ArrayList<>();
                        
                        if(this.isDebug) System.out.println("hash add to nodes.");
                        for(String hash : hashes) {
                            if(this.isDebug) System.out.println(hash + " -> " + BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(hash))));
                            nodes.add(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(hash)));
                        }
                        
                        if(nodes.size()%2 > 0 && nodes.size() > 2) {
                            if(this.isDebug) System.out.println("init node length is odd, size is bigger than 2, so add last item again for even.");
                            nodes.add(BlockChainUtils.getReversedByteArray(BlockChainUtils.hexStringToByteArray(hashes[hashes.length-1])));
                        }
                        ArrayList<ArrayList<byte[]>> layers = new ArrayList<>();//merkle path
                        layers.add(nodes);
                        ArrayList<byte[]> newNodes = null;
                        while(nodes.size() > 1) {
                            if(this.isDebug) System.out.println("merklet tree merging loop!");
                            newNodes = new ArrayList<>();
                            for(int i=0; i<nodes.size(); i+=2) {
                                String temp = BlockChainUtils.byteArrayToHexString(nodes.get(i)) + BlockChainUtils.byteArrayToHexString(nodes.get(i+1));
                                if(this.isDebug) System.out.println(BlockChainUtils.byteArrayToHexString(nodes.get(i)) + " + " + BlockChainUtils.byteArrayToHexString(nodes.get(i+1)) + " = " + temp);
                                byte[] hashing2temp = BlockChainUtils.encSHA256(BlockChainUtils.encSHA256(BlockChainUtils.hexStringToByteArray(temp)));
                                if(this.isDebug) System.out.println(temp + " -> hashing 2 times -> " + BlockChainUtils.byteArrayToHexString(hashing2temp));
                                newNodes.add(hashing2temp);
                            }
                            if(newNodes.size()%2 > 0 && newNodes.size() > 2) {
                                if(this.isDebug) System.out.println("new node length is odd, size is bigger than 2, so add last item again for even.");
                                newNodes.add(newNodes.get(newNodes.size()-1));
                            }
                            nodes = newNodes;
                            layers.add(nodes);
                        }
                        String calculatedMerkleRoot = BlockChainUtils.byteArrayToHexString(BlockChainUtils.getReversedByteArray(nodes.get(0)));
                        String givenMerkleRoot = BlockChainUtils.byteArrayToHexString(header.merkletRootHash);
                        
                        if(this.isDebug) System.out.println("given merkle root      : " + givenMerkleRoot);
                        if(this.isDebug) System.out.println("calculated merkle root : " + calculatedMerkleRoot);
                                
                        if(!calculatedMerkleRoot.equals(givenMerkleRoot)) {
                            throw new AssertionError();
                        }
                    } catch (NoSuchAlgorithmException e) {
                        e.printStackTrace();
                    }
                }//END OF FUNCTION  
            }//END OF CLASS
//         
//        if __name__ == '__main__':
//            merkle_path(5)
            new MerkleTree(hashes, merkleRootHash).merkle_path(5);
            
//            5번째 인덱스의 merkle path :
//                ['fb8b5c47e9693f9834c9704460c4f8358c12ecf8ea2f0322ee2cad4f2a027618', 
//                '8278e3e47bb6f0e7c0cadfc3ab6f395689251df1e31922f5e7c9851eae356290', 
//                'aafcb72ae5c43b5ab2e1fd20e91381aee67cc3363f87bc8e4ae2504dc9272316', 
//                '5e22d508e7ff865131de483a95afc02c75cf0cbeb763b705e4c7d68afa9d7386']
        }
        
        if(CALCULATE_THRESHOLD) {
            
            /*
            target threshold is a 256-bits unsigned integer which a header hash must be equal to or below in order for that header to be a valid part of the block chain.
            0x 1b0404cb
            0x 1b 0404cb
            0404cb      * 256  ^ (1b       - 3)
            significant * base ^ (exponent - bytets in significant)
            0x 1bc330000000000000000000000000000000000000000000
            byte length : 0x18 : 24
            */
            
            Long nBits = 0x181bc330L;
            Long significant = (nBits&0x00ffffff);
            Long exponent = (nBits>>>24);
            Long base = 256L;
            System.out.println("significant : " + BlockChainUtils.numericToHexString(significant, 4) + " : " + significant);
            System.out.println("exponent    : " + BlockChainUtils.numericToHexString(exponent, 4) + " : " + exponent);
            
            
            BigDecimal bdSignificant = BigDecimal.valueOf(significant);
            BigDecimal bdExponent = BigDecimal.valueOf(exponent);
            BigDecimal bdBase = BigDecimal.valueOf(base);
            System.out.println("Integer.MAX_VALUE : " + Integer.MAX_VALUE);
            System.out.println("Long.MAX_VALUE : " + Long.MAX_VALUE);
            
            BigDecimal bdThreshold = bdSignificant.multiply(bdBase.pow(bdExponent.subtract(BigDecimal.valueOf(3L)).intValue()));
            System.out.println("TRHESHOLD : " + bdThreshold);
            System.out.println("TRHESHOLD : " + BlockChainUtils.numericToHexString(bdThreshold, 24));
            System.out.println("TRHESHOLD : " + BlockChainUtils.byteArrayToHexString(BlockChainUtils.getTargetFromNbits(nBits)));
        }
        
        if(MINING_EXAMPLE) {
            /**
             * Attack at 9PM!
             * 위의 텍스트에 숫자를 붙인뒤 SHA-256 해쉬를 돌렸을때, 앞에 3바이트가 0이 나오는 텍스트찾기
             */
            String msg = "Attack at 9PM!";
            String testMsg = "";
            System.out.println(BlockChainUtils.byteArrayToHexString(BlockChainUtils.encSHA256(msg.getBytes()))); //c53ae0b1db6f94ce4177112d386c8bdf2f1b1d949f5a19f47d473a785dc97afc
            int i = 0;
            while(true) {
                testMsg = msg+String.valueOf(i);
                byte[] tmpBytes = BlockChainUtils.encSHA256(testMsg.getBytes());
                System.out.println("index " + i + " : " + BlockChainUtils.byteArrayToHexString(tmpBytes));
                if(tmpBytes[0] == 0x00 && tmpBytes[1]>>>4 == 0x00) {
                    break;
                }
                i++;
            }
            if(testMsg.length() > 0)
                System.out.println("FIND SUCCESS : " + testMsg);
            else 
                System.out.println("FIND FAIL : " + testMsg);
        }
        
        if(OLD_CASE) {
            //140402 index bitcoin block hashing check
            String[] transactions = {"this is first transaction"};
            BlockHeader blockHeader1 = new BlockHeader();
            blockHeader1.version = 1L;
            blockHeader1.previousBlockHash = BlockChainUtils.hexStringToByteArray("00000000000008a3a41b85b8b29ad444def299fee21793cd8b9e567eab02cd81");
            blockHeader1.merkletRootHash = BlockChainUtils.hexStringToByteArray("2b12fcf1b09288fcaff797d71e950e71ae42b91e8bdb2304758dfcffc2b620e3");
            blockHeader1.timestamp = 1305998791L;
            blockHeader1.nBits = 440711666L;
            blockHeader1.nonce = 2504433986L;
            Block block1 = new Block(blockHeader1, transactions);
            System.out.println(BlockChainUtils.byteArrayToHexString((block1.getBlockHash())).equals("00000000000000001e8d6829a8a21adc5d38d0a473b144b6765798e61f98bd1d"));
            
            //1661597 index bitcoin block hashing check
            String[] transactions2 = {"this is second transaction"};
            BlockHeader blockHeader2 = new BlockHeader();
            blockHeader2.version = 536870912L;
            blockHeader2.previousBlockHash = BlockChainUtils.hexStringToByteArray("0000000000000000006a5efe4a18b81bbabdd19f3633a5d4f06159fd6c7b69b2");
            blockHeader2.merkletRootHash = BlockChainUtils.hexStringToByteArray("a7469fd328050e44a2d7b19a5cff740a26b245acc1a6336ed4edfa0274ad0c8a");
            blockHeader2.timestamp = 1514521239L;
            blockHeader2.nBits = 402691653L;
            blockHeader2.nonce = 380969149L;
            Block block2 = new Block(blockHeader2, transactions2);
            System.out.println(BlockChainUtils.byteArrayToHexString((block2.getBlockHash())).equals("00000000000000000079ecf3ba985ba5f769441511ca612ee9e289c424b790c1"));
        }
        
    }//END OF FUNCTION
}//END OF CLASS
